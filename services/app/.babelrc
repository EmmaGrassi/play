        "es2016",
        "es2017",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "react-hot-loader/babel"
      ]
    },

    "production": {
      "presets": [
        "es2015",
        "es2016",
        "es2017",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  }
}
